<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SPID"
        x:Class="SPID.MainWindow"
        mc:Ignorable="d"
        Title="SPID Plugin - Functions" UseLayoutRounding="True" Width="800" Height="600">
    <Grid>
        <ListBox x:Name="listBox" Width="800" SelectionChanged="listBox_SelectionChanged"/>
        <Label x:Name="status" VerticalAlignment="Bottom"></Label>
    </Grid>
</Window>
